    "apca-w3": "^0.1.9",
    "chroma-js": "2.4.2",
    "clsx": "^2.1.1",
    "lodash.round": "^4.0.4",
    "lodash.throttle": "^4.1.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@craco/types": "^7.1.0",
    "@types/apca-w3": "^0.1.3",
    "@types/chroma-js": "^2.4.4",
    "@types/chrome": "0.0.268",
    "@types/lodash.round": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^20.14.9",
